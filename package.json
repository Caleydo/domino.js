{
  "name": "domino.js",
  "version": "0.0.1",
  "peerDependencies": {
    "caleydo_web": "*",
    "caleydo_selectioninfo": "*",
    "caleydo_links": "*"
  },
  "caleydo": {
    "plugins": {
      "web": {
        "type": "app",
        "name": "Domino.js"
      }
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.2",
        "bootstrap": "~3.3.4"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": [
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  }
}

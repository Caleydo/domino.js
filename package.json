{
  "name": "domino.js",
  "version": "0.0.1",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_links": "*",
    "caleydo_vis": "*"
  },
  "caleydo": {
    "plugins": {
      "web": {
        "type": "app",
        "name": "Domino.js"
      }
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "bootstrap": "~3.3.4"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "shim": {
        "bootstrap": [
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://dev.caleydo.org/nexus/content/repositories/caleydo-web/"
  }
}
